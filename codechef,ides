import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();  // Read the day in March
        
        if (N == 15)
            System.out.println("Yes");
        else
            System.out.println("No");

	}
}
