class Solution {

    void printTriangle(int n) {
        for(int row=1; row<=n; row++){
            for(int sp=1; sp<=n-row; sp++){
                System.out.print(" ");
            }
            for(int col=1; col<=2*row-1; col++){
                System.out.print("*");
            }
            System.out.println();
        }
    }
}
